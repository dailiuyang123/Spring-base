<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.daily.mybatis.dao.UserDao">


    <select id="findAll" resultType="com.daily.mybatis.entity.User">

        select  id,name from t_user

    </select>

    <select id="selcectbyUser"  parameterType="com.daily.mybatis.entity.User" resultType="com.daily.mybatis.entity.User">
        SELECT  *  from t_user WHERE  name=#{name} AND  password=#{password}
    </select>


</mapper>
